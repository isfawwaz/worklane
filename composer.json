{
    "name": "isfawwaz/wp-installer",
    "description": "Wordpress client installer",
    "require": {
        "php": "^7.2.9",
        "ext-zip": "*",
        "guzzlehttp/guzzle": "^6.0",
        "symfony/console": "^4.0|^5.0",
        "symfony/filesystem": "^4.0|^5.0",
        "symfony/process": "^4.2|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmad Faris",
            "email": "faris@paperplane.id"
        }
    ],
    "bin": [
        "bin/worklane"
    ],
    "autoload": {
        "psr-4": {
            "Worklane\\Installer\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Worklane\\Installer\\Console\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.1-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "suggest": {
        "wp-cli": "For manage and utilizing the project"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
